

{
  "plugins": ["prettier"],

  "extends": ["plugin:prettier/recommended"],
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "node": true
  },

  "rules": {
    "no-new": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-alert": "off",
    "no-undef": "off",
    "import/extensions": "off",
    "prefer-template": "off",
    // "max-depth": ["error", 2],
    // "max-lines-per-function": ["error", 15],
    "eol-last": ["error", "always"]
  }
}


